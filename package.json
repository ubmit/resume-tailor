{
  "name": "resume-tailor",
  "version": "1.0.0",
  "description": "CLI to tailor resumes for specific job positions",
  "type": "module",
  "bin": {
    "resume-tailor": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "prepare:prompt": "tsx src/index.ts prepare",
    "generate": "tsx src/index.ts generate",
    "extract": "tsx src/index.ts extract",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "commander": "^14.0.2",
    "pdf-parse": "^2.4.5"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/pdf-parse": "^1.1.5",
    "@vitest/coverage-v8": "^4.0.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
